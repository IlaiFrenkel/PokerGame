<Window x:Class="PokerGame_UI.PlayersNamesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PokerGame_UI"
        mc:Ignorable="d"
        Title="PlayersNamesWindow" Height="330.667" Width="525.333">
    <Grid Margin="0,0,-6.333,0.333">
        <Label x:Name="lableCurrentPlayerName" Content="Player Name:" HorizontalAlignment="Left" Margin="128,76,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.277,0.577"/>
        <TextBox x:Name="TextBoxPlayerName" HorizontalAlignment="Left" Height="23" Margin="312,79,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonSubmitPlayerName" Content="Add" HorizontalAlignment="Left" Margin="185,140,0,0" VerticalAlignment="Top" Width="176" Click="buttonSubmitPlayerName_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="208,198,0,0" VerticalAlignment="Top" Width="124" Click="buttonCancel_Click"/>
        <ToolBarPanel HorizontalAlignment="Left" Height="100" Margin="250,269,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
